{
  "name": "api-node-typescript",
  "version": "1.0.0",
  "description": "Typescript NodeJS Api",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:dev": "ts-node-dev --transpile-only src/index.ts",
    "start": "npm run build && node dist/index.js",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeovanneCallegaro/api-nodejs-typescript.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Geovanne Callegaro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GeovanneCallegaro/api-nodejs-typescript/issues"
  },
  "homepage": "https://github.com/GeovanneCallegaro/api-nodejs-typescript#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "module-alias": "^2.2.2"
  }
}
